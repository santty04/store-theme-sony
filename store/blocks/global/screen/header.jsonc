{
  "header": {
    "title": "header",
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ],
    "props": {
      "blockClass": "header"
    }
  },
  "header.full": {
    "title": "header full",
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ],
    "props": {
      "blockClass": "header__full"
    }
  },

  "header-layout.mobile": {
    "title": "header mobile",
    "children": [
      "sticky-layout#mobile"
    ],
    "props": {
      "blockClass": "header__mobile"
    }
  },
  "sticky-layout#mobile": {
    "title": "sticky layout header mobile",
    "children": [
      "flex-layout.row#mobile"
    ],
    "props": {
      "blockClass": "sticky__layout--mobile"
    }
  },
  "flex-layout.row#mobile": {
    "title": "flex row header mobile",
    "children": [
      "flex-layout.col#mobile__drawer",
      "flex-layout.col#mobile__logo",
      "search-bar#mobile__header--search",
      "rich-text#desktop__header--help",
      "overlay-trigger",
      "flex-layout.col#mobile__minicart"
    ],
    "props": {
      "blockClass": "main-header-mobile",
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "colSizing": "auto"
    }
  },
  "search-bar#mobile__header--search": {
    "props": {
      "compactMode": false
    }
  },
  "flex-layout.col#mobile__drawer": {
    "title": "flex mobile drawer",
    "children": [
      "drawer"
    ],
    "props": {
      "width": "14%",
      "preventVerticalStretch": true,
      "blockClass": "drawer__mobile"
    }
  },
  "flex-layout.col#mobile__logo": {
    "title": "flex logo desktop",
    "children": [
      "logo#mobile__logo"
    ],
    "props": {
      "blockClass": "col__mobile--logo",
      "verticalAlign": "middle"
    }
  },
  "flex-layout.col#mobile__minicart": {
    "title": "flex minicart desktop",
    "children": [
      "minicart.v2"
    ]
  },
  "logo#mobile__logo": {
    "title": "logo",
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "assets/img/logo.png",
      "width": 103,
      "height": 18,
      "blockClass": "logo__desktop"
    }
  },
  "drawer": {
    "title": "drawer",
    "children": [],
    "props": {
      "blockClass": "drawer__mobile"
    }
  }
}
