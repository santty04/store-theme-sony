      "image#phone__cinema--image",
      "rich-text#phone__cinema--title"
    ],
    "props": {
      "blockClass": "section__phone--cinema"
    }
  },
  "image#phone__cinema--image": {
    "title": "cinema image",
    "props": {
      "src": "assets/img/favorite-cinema.webp",
      "link": {
        "url": "/"
      },
      "blockClass": "image__phone--sony"
    }
