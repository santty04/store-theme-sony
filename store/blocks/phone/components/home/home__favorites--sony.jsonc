{
  "flex-layout.row#phone__section--favorites-1": {
    "title": "phone favorites sony 1",
    "children": [
      "flex-layout.col#phone__favorites--cinema",
      "flex-layout.col#phone__favorites--cams"
    ],
    "props": {
      "blockClass": "phone__favorites--one",
      "preserveLayoutOnMobile": true
    }
  },
  "flex-layout.row#phone__section--favorites-2": {
    "title": "phone favorites sony 2",
